<br />
<% @places.each do |place| %>
  <div class = "booyah-box col-10 offset-1 place-box">
    <h1><%= place.name %></h1>
    <i><%= place.address %></i><br />
    <p><%= place.description %> </p>

    <% if place.user.present? %>
    <small><%= place.user.email %></small>
    <% end %>
  </div>
  <br />
<% end %>
<%= will_paginate @places, :class => 'apple_pagination' %>